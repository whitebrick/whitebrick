  "dependencies": {
    "@auth0/auth0-react": "^1.5.0",
    "@lifeomic/axios-fetch": "^2.0.2",
    "ag-grid-community": "^26.0.0",
    "ag-grid-enterprise": "^26.0.1",
    "ag-grid-react": "^26.0.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "evergreen-ui": "^6.4.0",
    "formik": "^2.2.9",
