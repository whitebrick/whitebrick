  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "dotenv": "^8.6.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.2.1",
    "typescript": "^4.3.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "ACTIVE_ENV='prod' gatsby build",
    "build:staging": "ACTIVE_ENV='staging' gatsby build",
    "build:development": "ACTIVE_ENV='development' gatsby build",
    "develop": "gatsby develop",
    "develop:staging": "ACTIVE_ENV='staging' gatsby develop",
    "develop:prod": "ACTIVE_ENV='prod' gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ignore-path .eslintignore . --ext js --ext ts --ext tsx",
    "lint:fix": "eslint --ignore-path .eslintignore . --ext js --ext ts --ext tsx --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare": "husky install",
    "postinstall": "husky install .github/husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --ignore-path .eslintignore . --ext js --ext ts --ext tsx --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whitebrick/whitebrick"
  },
  "bugs": {
    "url": "https://github.com/whitebrick/whitebrick/issues"
  }
}
