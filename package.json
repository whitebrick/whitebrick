    "react-avatar": "^3.10.0",
    "react-contexify": "^5.0.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.2.0",
    "react-modal": "^3.13.1",
